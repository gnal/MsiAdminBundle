{% for i in 0..object.lvl %}
    {% if 0 != i %}<span class="badge"><i class="icon-chevron-right icon-white"></i></span>{% endif %}
{% endfor %}
{% if column.options.edit %}
    <a href="{{ admin.genUrl('edit', {'id': object.id}) }}">{{ column.value }}</a>
{% else %}
    {{ column.value }}
{% endif %}
